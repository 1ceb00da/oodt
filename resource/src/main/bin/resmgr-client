# $Id$


if [ -z $JAVA_HOME ] ; then
	JAVA_HOME=/path/to/java/home
else
	JAVA_HOME=${JAVA_HOME}
fi

export JAVA_HOME

$JAVA_HOME/bin/java -Djava.ext.dirs=../lib -Dgov.nasa.jpl.oodt.cas.resource.properties=../etc/resource.properties -Djava.util.logging.config.file=../etc/logging.properties gov.nasa.jpl.oodt.cas.resource.system.XmlRpcResourceManagerClient $*
