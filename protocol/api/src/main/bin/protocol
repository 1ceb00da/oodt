#!/bin/csh

set args

if ( $#args > 0 ) then
	set args = "$*"
endif

${JAVA_HOME}/bin/java \
	-Djava.ext.dirs=../lib \
	-Djava.util.logging.config.file=../etc/logging.properties \
	-Dorg.apache.oodt.cas.protocol.manager.config.file=../policy/protocol-config.xml \
   	org.apache.oodt.cas.protocol.system.CLProtocolManager ${args}